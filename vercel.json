{
   "version": 2,
   "builds": [
    {
        "src": "./index.js",
        "use": "@vercel/node"
    }
   ],
   "routes": [
    {
        "src": "/(.*)",
        "dest": "/"
    }
   ],
   "functions": {
    "api/*": {
      "runtime": "vercel-node@2",
      "maxHttpBufferSize": "50MB"
    }
  }
}